{
  "name": "snape",
  "version": "0.1.3",
  "description": "A web application to search and stream torrents.",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint {shared,pages,server}/**/*.js",
    "lintfix": "eslint --fix {shared,pages,server}/**/*.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "electron .",
    "start": "NODE_ENV=production npm run build:client && electron .",
    "build:client": "next build",
    "build": "npm run build:client && electron-packager . --out=package --darwin --overwrite",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint --write {shared,pages,server}/**/*.js",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritz078/snape.git"
  },
  "keywords": [
    "torrent",
    "webtorrent",
    "stream",
    "download",
    "video"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/snape/issues"
  },
  "homepage": "https://github.com/ritz078/snape#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "dependencies": {
    "atob": "^2.0.3",
    "autoprefixer": "^6.7.7",
    "babel-register": "^6.24.1",
    "bytebuffer": "^5.0.1",
    "chai": "^3.5.0",
    "chromecasts": "^1.9.0",
    "classnames": "^2.2.5",
    "cors": "^2.8.3",
    "debug": "^2.6.4",
    "deep-copy": "^1.2.0",
    "electron": "^1.6.5",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "express": "4.15.2",
    "get-port": "^3.1.0",
    "imdb-api": "^2.2.2",
    "internal-ip": "^1.2.0",
    "is-electron-renderer": "^2.0.1",
    "just-is-empty": "^1.0.1",
    "mime": "^1.3.4",
    "next": "2.1.1",
    "next-redux-wrapper": "1.0.0",
    "parse-torrent": "^5.8.3",
    "parse-torrent-name": "^0.5.4",
    "plyr": "^2.0.12",
    "postcss-easy-import": "^2.0.0",
    "pretty-bytes": "^4.0.2",
    "prop-types": "^15.5.8",
    "protobufjs": "^6.7.3",
    "pump": "^1.0.2",
    "query-string": "^4.3.4",
    "range-parser": "^1.2.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-loading-bar": "^0.0.4",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-toastify": "^1.3.0",
    "react-virtualized": "^9.7.3",
    "redux": "^3.6.0",
    "redux-create-reducer": "^1.1.1",
    "redux-observable": "^0.14.1",
    "rimraf": "^2.5.4",
    "rxjs": "^5.3.0",
    "sinon": "^2.1.0",
    "spectre.css": "^0.2.13",
    "styled-components": "^1.4.5",
    "tempy": "^0.1.0",
    "thepiratebay": "^1.4.0",
    "url-loader": "^0.5.8",
    "webtorrent": "^0.98.18",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-polyfill": "6.23.0",
    "colors": "1.1.2",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.0.0",
    "devtron": "^1.4.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "husky": "^0.13.3",
    "jest": "19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "lint-staged": "^3.4.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.3",
    "prettier-eslint-cli": "^3.4.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "rimraf": "2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
